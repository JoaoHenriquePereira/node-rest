{
    "title": "compute api input schema v1",
    "type": "object",
    "required": ["graph_type", "graph"],
    "properties": {
        "graph_type": {
            "type": "string"
        },
        "graph": {
            "type": "array",
            "items": {
                "type": "object",
                "minItems": 10,
                "uniqueItems": true,
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "coordinates": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "options": {
            "type": "object",
            "properties": {
                "mutation_rate": {
                    "type": "number",
                    "minimum": "0.0",
                    "maximum": "1.0"
                },
                "elitism": {
                    "type": "boolean"
                },
                "population_size": {
                    "type": "integer",
                    "minimum": "5",
                    "maximum": "3000"
                },
                "tournament_size": {
                    "type": "integer",
                    "minimum": "5",
                    "maximum": "3000"
                }
            }
        }
    }
}